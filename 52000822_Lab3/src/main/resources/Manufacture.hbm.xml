<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Lab03_52000822.POJO.Manufacture" table="manufacture" schema="hibernate">
        <id name="id">
            <column name="id" sql-type="int"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(120)" length="120"/>
        </property>
        <property name="location">
            <column name="location" sql-type="varchar(120)" length="120"/>
        </property>
        <property name="employee">
            <column name="employee" sql-type="int"/>
        </property>
    </class>
</hibernate-mapping>