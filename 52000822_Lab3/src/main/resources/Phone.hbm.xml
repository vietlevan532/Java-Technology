<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Lab03_52000822.POJO.Phone" table="phone" schema="hibernate">
        <id name="id">
            <column name="id" sql-type="int"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(120)" length="120"/>
        </property>
        <property name="price">
            <column name="price" sql-type="int"/>
        </property>
        <property name="color">
            <column name="color" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="country">
            <column name="country" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="quantity">
            <column name="quantity" sql-type="int" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>